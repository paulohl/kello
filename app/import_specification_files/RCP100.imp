   Object D-MOVTO
       Type ENTRY-FIELD
       Parent PRINCIPAL
       Stylename {font-001}
       Start (404,148)
       Size  (336,56)
       Masterfield DATA-MOVTO
       Style AUTOSCROLL BORDER BLANK-WHEN-ZERO RESIZE-CHAR('W') AUTOSWIPE
       Picture ZZ/ZZ/ZZZZ
       Dialog CASE(OFF)
           Event TAB
               BRANCH-TO-PROCEDURE OPCAO-SELECIONADA ;
           End Event # TAB
           Event CR
               BRANCH-TO-PROCEDURE OPCAO-SELECIONADA ;
           End Event # CR
           Procedure OPCAO-SELECIONADA
               VALIDATE D-MOVTO ;
               MOVE 0 ALTERA ;
               ENABLE-OBJECT PRINCIPAL ;
               SET-FLAG VERIFICA-PERMISSAO(1) ;
               RETC ;
               XIF= LIBERADO 1 DESABILITA-GRAVAR ;
               SET-FOCUS EF9 ;
           End Procedure # OPCAO-SELECIONADA
           Event VAL-ERROR
               DISABLE-OBJECT PRINCIPAL ;
               ENABLE-OBJECT D-MOVTO ;
               SET-FOCUS D-MOVTO ;
               INVOKE-MESSAGE-BOX MBOX-ERROS ERR-MSG $EVENT-DATA ;
               RETC ;
           End Event # VAL-ERROR
       End Dialog
   End Object  #D-MOVTO

