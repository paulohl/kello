               DELETE-LIST-ITEM LB1 CONT1 1 ;
           End Event # DEL
           Procedure NADA
               RETC ;
           End Procedure # NADA
           Procedure FOCAR-CRITICA
               SET-FOCUS LB1 ;
           End Procedure # FOCAR-CRITICA
       End Dialog
