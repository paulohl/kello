   Object EF71
       Type ENTRY-FIELD
       Parent PRINCIPAL
       Stylename {font-002}
       Start (1984,148)
       Size  (144,56)
       Masterfield PORTADOR
       Style AUTOSCROLL BORDER FIT-PICTURE RESIZE-CHAR('Z') AUTOSWIPE
       Picture ZZZZ
       Dialog CASE(OFF)
           Event CR
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # CR
           Event TAB
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # TAB
           Procedure OPCAO
               MOVE 0 FLAG-CRITICA ;
               SET-FLAG LE-PORTADOR(1) ;
               RETC ;
               IF= FLAG-CRITICA 1 FOCAR-CRITICA ;
               REFRESH-OBJECT EF71 ;
               REFRESH-OBJECT EF72 ;
               SKIP-TO-NEXT-OBJECT 1 ;
           End Procedure # OPCAO
           Event F2
               MOVE 0 FLAG-CRITICA ;
               SET-FLAG POPUP-PORTADOR(1) ;
               RETC ;
               IF= FLAG-CRITICA 1 FOCAR-CRITICA ;
               REFRESH-OBJECT EF71 ;
               REFRESH-OBJECT EF72 ;
               SKIP-TO-NEXT-OBJECT 1 ;
           End Event # F2
           Procedure FOCAR-CRITICA
               SET-FOCUS EF71 ;
           End Procedure # FOCAR-CRITICA
       End Dialog
   End Object  #EF71

