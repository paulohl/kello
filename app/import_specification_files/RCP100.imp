           CREATE-WINDOW WIN1 ;
           CREATE-WINDOW WIN4 ;
       *
           SET-FLAG CENTRALIZA(1) ;
           RETC ;
       *
           MOVE 0 ALTERA ;
           DISABLE-OBJECT PRINCIPAL ;
           ENABLE-OBJECT D-MOVTO ;
           SET-FOCUS D-MOVTO ;
           RETC ;
       End Event # SCREENSET-INITIALIZED
       Procedure CARREGAR-DATA
           REFRESH-OBJECT PRINCIPAL ;
           SET-FOCUS D-MOVTO ;
           RETC ;
