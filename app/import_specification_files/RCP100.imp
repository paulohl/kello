   Object PB4
       Type PUSH-BUTTON
       Parent PRINCIPAL
       Stylename {font-001}
       Start (2604,1336)
       Size  (484,88)
       Display "~Inclui Cheque"
       Dialog CASE(OFF)
           Event BUTTON-SELECTED
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # BUTTON-SELECTED
           Event CR
               BRANCH-TO-PROCEDURE OPCAO ;
           End Event # CR
           Procedure OPCAO
               ENABLE-OBJECT EF17 ;
               ENABLE-OBJECT EF18 ;
               MOVE 0 FLAG-CRITICA ;
               SET-FLAG LER-BANCO(1) ;
               RETC ;
               IF= FLAG-CRITICA 1 FOCAR-CRITICA ;
               MOVE TIPO-CH TIPO ;
               IF= TIPO "6" VER-CARTAO ;
               MOVE 0 CARTAO ;
               MOVE 0 TAXA-ADMINIST-CREDITO ;
               MOVE " " DESC-CARTAO ;
               REFRESH-OBJECT EF57 ;
               REFRESH-OBJECT EF58 ;
               VALIDATE EF14 ;
               SET-FLAG INCLUI-CHEQUE(1) ;
               RETC ;
               EXECUTE-PROCEDURE CLEAR-CHEQUE ;
               SET-FOCUS EF14 ;
           End Procedure # OPCAO
           Event CTPD
               VALIDATE EF14 ;
               SET-FLAG ADICIONA-CHEQUE(1) ;
               RETC ;
               REFRESH-OBJECT PRINCIPAL ;
               SET-FOCUS PB4 ;
           End Event # CTPD
           Event VAL-ERROR
               SET-FOCUS $EVENT-DATA ;
               INVOKE-MESSAGE-BOX MBOX-ERROS ERR-MSG $EVENT-DATA ;
               RETC ;
           End Event # VAL-ERROR
           Procedure VER-CARTAO
               IF= CARTAO 0 CHAMAR-CARTAO ;
               VALIDATE EF14 ;
               SET-FLAG INCLUI-CHEQUE(1) ;
               RETC ;
               EXECUTE-PROCEDURE CLEAR-CHEQUE ;
               SET-FOCUS EF14 ;
           End Procedure # VER-CARTAO
           Procedure CHAMAR-CARTAO
               SHOW-WINDOW WIN2 ;
               DISABLE-OBJECT PB5 ;
               SET-FOCUS EF57 ;
           End Procedure # CHAMAR-CARTAO
           Procedure FOCAR-CRITICA
               SET-FOCUS EF18 ;
           End Procedure # FOCAR-CRITICA
       End Dialog
   End Object  #PB4

   Object LB1
       Type LIST-BOX
